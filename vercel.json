{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/swagger.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/v1/api/(route.js)",
      "dest": "/api/$1"
    },
    {
      "src": "/(index.js)",
      "dest": "/public/$1"
    }
  ]
}